<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dev.freamarker.dao.FileDao">
    <select id="insertFile" parameterType="java.util.Map">
		insert into t_dev_file(id,content,moduleid,status,extend)
			values(#{id},#{content},#{moduleid},#{status},#{extend})
    </select>
</mapper>