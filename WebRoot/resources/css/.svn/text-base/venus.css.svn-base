/*************************详情样式************************/

.detail-info {
    display: table;
    width: 100%;
    margin: 0 auto;
    border: 1px solid #DCEBF7
}
.detail-info-item{
	margin:0px;
	padding:0px;
}
.detail-info-label {
    text-align: right;
    padding: 6px 10px 6px 4px;
    font-weight: 400;
    color: #667E99;
    background-color: transparent;
    border-top: 1px dotted #D5E4F1;
    display: table-cell;
    width: 120px;
    max-width:120px;
    min-width:120px;
    vertical-align: middle;
    color: #336199;
    background-color: #EDF3F4;
    border-top: 1px solid #F7FBFF;
    white-space: nowrap;
    text-overflow: ellipsis;
    overflow: hidden;
}

.detail-info-text {
    display: table-cell;
    padding: 6px 4px 6px 6px;
    border-bottom: 1px dotted #D5E4F1;
    padding-left: 12px;
    width:100%;
    WORD-BREAK: break-all;
    WORD-WRAP: break-word;
}


.detail-info-text>span+span:before {
    display: inline;
    content: ",";
    margin-left: 1px;
    margin-right: 3px;
    color: #666;
    border-bottom: 1px solid #FFF
}

.detail-info-item:first-child .detail-info-label,
.detail-info-item:first-child .detail-info-text {
    border-top: none;
}

