/************************ select disabled ********************************/
select:disabled+div{
	opacity:1!important;
}
select:disabled+div>a {
    background-color: #eee!important;
    cursor: not-allowed!important;
}
select:disabled+div>a:focus {
	outline:none;
}
/************************让文件域显示路劲**************************/
.show-file-path:after{
	content:attr(data-path) !important;
}
/************************模糊/高级查询转换开关**********************/
input[type=checkbox].ace.ace-switch.search-switch+.lbl:before {
    content: "模糊查询\a0\a0\a0\a0高级查询";
    width: 82px;
    text-indent: 5px;

    height: 34px;
    line-height: 34px;
}
input[type=checkbox].ace.ace-switch.search-switch:checked+.lbl:before {
    content: "模糊查询\a0\a0\a0\a0高级查询";
    width: 82px;
    text-indent: -33px;
    height: 34px;
    line-height: 34px;
}
input[type=checkbox].ace.ace-switch.search-switch+.lbl:after {
    left:60px;
    height: 30px;
    line-height: 30px;
}
input[type=checkbox].ace.ace-switch.search-switch:checked+.lbl:after {
    left: 2px;
    height: 30px;
    line-height: 30px;
}
